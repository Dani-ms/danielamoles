{
  "name": "daniela",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Daniela Moles <danielams.designer@gmail.com>",
  "contributors": [
    ""
  ],
  "dependencies": {
    "@react-spring/addons": "^9.0.0-beta.33",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.12.0",
    "gatsby": "^2.25.0",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.2.3",
    "gatsby-plugin-sharp": "^2.12.1",
    "gatsby-plugin-sitemap": "^2.3.4",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-react-router-scroll": "^3.0.14",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-csv": "^2.2.3",
    "gatsby-transformer-sharp": "^2.10.1",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "slugify": "^1.4.0",
    "typescript-plugin-css-modules": "^3.0.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/uuid": "^7.0.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-check-updates": "^4.1.2",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf _graphql-generated_ .cache public",
    "build": "npm run clean && gatsby build",
    "deploy": "gh-pages -d public",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,scss,sass,css,md}\"",
    "generate:config:release": "rimraf __config.release && node scripts/generate-config/generate-config.js",
    "lint:typescript": "tsc",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "analyze:last-build-size": "webpack-bundle-analyzer public/webpack-bundle-analyzer-stats.json",
    "npm-check-updates": "npm-check-updates"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:typescript"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,scss,sass,css,md}": "prettier --write"
  },
  "main": "bootstrap-build-environment.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dani-ms/danielamoles"
  },
  "bugs": {
    "url": "https://github.com/Dani-ms/danielamoles/issues"
  },
  "homepage": "https://dani-ms.github.io/danielamoles"
}
